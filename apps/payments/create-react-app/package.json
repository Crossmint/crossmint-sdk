{
    "name": "@crossmint/client-sdk-react-ui-starter",
    "version": "1.3.128",
    "private": true,
    "repository": "https://github.com/Crossmint/crossmint-sdk",
    "license": "Apache-2.0",
    "author": "Paella Labs Inc",
    "files": ["public", "src", ".env", "LICENSE", "package.json", "README.md", "tsconfig.json", "yarn.lock"],
    "scripts": {
        "build": "if [ \"$CI\" = \"true\" ]; then echo 'Skipping build in CI'; exit 0; else CI=false react-scripts build; fi",
        "eject": "react-scripts eject",
        "start": "react-scripts start"
    },
    "browserslist": {
        "production": [">0.2%", "not dead", "not op_mini all"],
        "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
    },
    "eslintConfig": {
        "extends": ["react-app"]
    },
    "dependencies": {
        "@crossmint/client-sdk-react-ui": "workspace:*",
        "eslint-config-react-app": "7.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-scripts": "5.0.1",
        "stream": "npm:stream-browserify@3.0.0",
        "web-vitals": "3.4.0"
    },
    "devDependencies": {
        "@types/react": "18.2.24",
        "@types/react-dom": "18.2.7",
        "typescript": "5.3.3"
    }
}
